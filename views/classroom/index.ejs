<h1>Classroom</h1>

<% listQuestions.forEach(function(q) { %>
    <h2>
      <a href="/question/view/<%= q.id %>">
        <%= q.title %>
      </a>
    </h2>
    <h3><%= q.category %></h3>
    <% if (q.users.length) { %>
        <h4>USER <%= q.users[0].fullName  %></h4>
    <% } %>
    <p><%= q.question %></p>
         
  </div>
<% }) %>

